script/source = "extends TileMap

const CACTUS = preload(\"res://scenes/Cactus.tscn\")
const SHROOM = preload(\"res://scenes/Mushroom.tscn\")

func generate():
	var size = Game.get_map().SIZE
	# Generate borders
	for x in range(size.y):
		var y = -1
		var position = Vector2(x, y)
		set_cellv(position, 7)
	for x in range(size.y):
		var y = size.y + 1
		var position = Vector2(x, y)
		set_cellv(position, 7)
	
	# Generate default tile
	for y in range(size.y):
		for x in range(size.x):
			var position = Vector2(x - size.x / 2, y - size.y / 2) * Vector2(8, 8) + Vector2(4, -8)
			var map = world_to_map(position)
			var rand = rand_range(0, 100)
			
			if rand < 10:
				set_cellv(map, 2)
			elif rand < 20:
				set_cellv(map, 6)
			else:
				set_cellv(map, 0)
	
	# Generate grass
	var noise = Game.generate_noise(size.x, size.y, 1, 20, 10)

	for y in noise.size():
		var col = noise[y]
		for x in col.size():
			var row = col[x]

			if row < 0: 
				continue
			else:
				var position = Vector2(x - size.x / 2, y - size.y / 2) * Vector2(8, 8) + Vector2(4, -8)
				var map = world_to_map(position)
				set_cellv(map, 3)
				
				if rand_range(0, 100) < 2:
					set_cellv(map, 4)
					
				if rand_range(0, 100) < 0.5:
					set_cellv(map, 5)
					
				if rand_range(0, 100) < 1:
					var shroom = SHROOM.instance()
					get_parent().get_node(\"Resources\").add_child(shroom)
					shroom.transform.origin = position
				
func _ready():
	Game.LOADING_MAP = true
	generate()
	Game.LOADING_MAP = false
"

[sub_resource type="GDScript" id=3]
script/source = "extends YSort

const TREE = preload(\"res://scenes/Rock.tscn\")
const SHROOM = preload(\"res://scenes/Mushroom.tscn\")

func generate():
	var ground_map = get_node(\"/root/Zelda/Underworld/TileMap_Ground\")
	var size = Game.MAP_SIZE / Vector2(2, 2)
	var noise = Game.generate_noise(size.x, size.y, 10, 50, 10)

	for y in noise.size():
		var col = noise[y]
		for x in col.size():
			var row = col[x]

			if row < 0: 
				continue
			else:
				var position = Vector2(x - size.x / 2, y - size.y / 2) * Vector2(8, 8) + Vector2(4, -8)
				var map_position = ground_map.world_to_map(position)
				var cell = ground_map.get_cellv(map_position)
				
				if cell != 3: continue
				
				if rand_range(0, 100) < 1:
					var tree = SHROOM.instance()
					add_child(tree)
					tree.transform.origin = position
				else:
					var tree = TREE.instance()
					add_child(tree)
					tree.transform.origin = position

func _ready():
	Game.LOADING_RESOURCES = true
	generate()
	Game.LOADING_RESOURCES = false
"

